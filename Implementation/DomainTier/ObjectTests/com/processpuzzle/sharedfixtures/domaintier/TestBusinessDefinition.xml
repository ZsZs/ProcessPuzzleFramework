<?xml version="1.0" encoding="UTF-8"?>
<businessDefinition 
	xmlns = "http://www.processpuzzle.com/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.processpuzzle.com/ ../../business/definition/domain/BusinessDefinition.xsd ">

	<!-- PartyTypes: Defines acceptable types of parties. -->
	<partyTypes>
		<partyType name = "PersonType" />
		<partyType name = "CompanyType" />
		<partyType name = "XPProject" />
	</partyTypes>
	
	<!-- PartyRoleTypes: Defines the roles can be filled in the workflows. -->
	<partyRoleTypes>
		<partyRoleType name="ProcessPuzzleAdministrator">
			<description>Administrator of the ProcessPuzzle application.</description>
			<partyRoleConstraints>
				<partyRoleConstraint typeOfParty = "PersonType" />
			</partyRoleConstraints>
		</partyRoleType>
		
		<partyRoleType name="FatherRole">
			<description>Defines the role of father of childrens.</description>
			<partyRoleConstraints>
				<partyRoleConstraint typeOfParty = "PersonType" />
			</partyRoleConstraints>
		</partyRoleType>
		
		<partyRoleType name="SonRole">
			<description>Defines the role of being a child of someone.</description>
			<partyRoleConstraints>
				<partyRoleConstraint typeOfParty = "PersonType" />
			</partyRoleConstraints>
		</partyRoleType>
		
		<partyRoleType name="EmployingXPProject">
			<description>An XP project which employs team members.</description>
			<partyRoleConstraints>
				<partyRoleConstraint typeOfParty = "XPProject" />
			</partyRoleConstraints>
		</partyRoleType>

		<partyRoleType name="XPTeamMemeber">
			<description>Member of an XP team.</description>
			<partyRoleConstraints>
				<partyRoleConstraint typeOfParty = "PersonType" />
			</partyRoleConstraints>
		</partyRoleType>

		<partyRoleType name="Customer">
			<description>Customer of the development effort.</description>
			<partyRoleConstraints>
				<partyRoleConstraint typeOfParty = "PersonType" />
			</partyRoleConstraints>
		</partyRoleType>

		<partyRoleType name="Coach">
			<description>Informal leader of an XP team.</description>
			<partyRoleConstraints>
				<partyRoleConstraint typeOfParty = "PersonType" />
			</partyRoleConstraints>
		</partyRoleType>

		<partyRoleType name="Developer">
			<description>Software developer, member of an XP team.</description>
			<partyRoleConstraints>
				<partyRoleConstraint typeOfParty = "PersonType" />
			</partyRoleConstraints>
		</partyRoleType>
	</partyRoleTypes>
	
	<partyRelationshipTypes>
		<partyRelationshipType name="FatherSonRelationship">
			<partyRelationshipConstraints>
				<partyRelationshipConstraint clientRoleType="FatherRole" supplierRoleType="SonRole" />
			</partyRelationshipConstraints>
		</partyRelationshipType>
	</partyRelationshipTypes>
	
	<artifactTypes>
		<artifactTypeGroup name="SystemAdministration">
			<defaultAccessRights>
				<accessRight partyRoleType="Coach" canCreate="true" canDelete="true" canModify="true" canRead="true" />
				<accessRight partyRoleType="XPTeamMember" canCreate="true" canDelete="false" canModify="true" canRead="true" />
			</defaultAccessRights>
		
			<artifactType name="RootArtifactFolder" artifactClassName="com.processpuzzle.artifact.domain.RootArtifactFolder" createOnStartUp="false" group="SystemAdministration" isSingleton="true" isSystem="true" isVersionControlled="false" pessimisticLock="false" refreshOnDocumentActivation="true" refreshOnViewActivation="true">
