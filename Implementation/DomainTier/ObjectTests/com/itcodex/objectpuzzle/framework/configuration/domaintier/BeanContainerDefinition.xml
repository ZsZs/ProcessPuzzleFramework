<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="userRequestContextFactory" class="com.processpuzzle.user_session.domain.DefaultUserRequestContextFactory"/>
  <bean id="predefinedUsersLoader" class="com.itcodex.objectpuzzle.framework.application_security.domain.PredefinedUsersLoader"/>

  <!-- Entity factories -->
  <bean id="userFactory" class="com.itcodex.objectpuzzle.framework.application_security.domain.UserFactory" lazy-init="true"/>
  
</beans>